{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Bus":  {
    "Server": "kafka",
    "ScriptsTopicName": "scripts",
    "ResultsTopicName": "script-results",
    "ResultsConsumerGroup": "script-results-consumers"
  },
  "ScriptsDatabase":  {
    "ConnectionString": "mongodb://database",
    "DatabaseName": "Executor",
    "ScriptsCollectionName": "UserScripts",
    "ExecutionResultsCollectionName": "UserScriptResults"
  },
  "Cache": {
    "ConnectionString" : "cache"
  },
  "Jobs": {
    "RepeatIntervalInSeconds" : 1
  }
}
