{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "AllowedHosts": "*",
    "Bus":  {
        "Server": "localhost:9092",
        "ScriptsTopicName": "scripts",
        "ScriptsConsumerGroup" : "scripts-consumer",
